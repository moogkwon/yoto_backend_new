{
  "apps": [
    {
      "name": "yoto_dev",
      "script": "./server.js",
      "instances": "max",
      "exec_mode": "cluster",
      "env": {
        "watch": [
          "app",
          "config",
          "start",
          "server.js",
          ".env"
        ],
        "NODE_ENV": "development",
        "DEBUG": "mquery,socket"
      },
      "env_production": {
        "NODE_ENV": "production",
        "DEBUG": false
      }
    },
    {
      "name": "yoto_dev_notification",
      "script": "./ace",
      "args": "send:notification",
      "env": {
        "watch": [
          "app",
          "config",
          "start",
          "server.js",
          ".env"
        ],
        "NODE_ENV": "development",
        "DEBUG": false
      },
      "env_production": {
        "NODE_ENV": "production",
        "DEBUG": false
      }
    }
  ]
}
